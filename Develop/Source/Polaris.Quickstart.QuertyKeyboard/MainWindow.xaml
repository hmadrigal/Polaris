                <ContentControl Content="8"  Tag="*" Style="{StaticResource ButtonContent}"/>
                <ContentControl Content="*"  Tag="8" Style="{StaticResource ButtonContent}"/>
            </local:ShiftSensitiveKey.KeyDisplays>
